<?xml version="1.0"?>
<Root>
   <!-- Each Policy for one LBP printer config, add more Policies as you want -->
   <Policy>
      <!-- Indicates that the printer is the default printer or not, value should be true/false -->
      <DefaultPrinter></DefaultPrinter>
      <!-- A translation rule that specifies a range of IP address for client systems -->
      <!-- To specify IP addresses in a specific range, use ip_address - ip_address -->
      <!-- To specify all of the IP addresses in a specific subnet, use ip_address/subnet_mask_bits -->
      <IPRange></IPRange>
      <!-- A translation rule that specifies a computer name -->
      <ClientName></ClientName>
      <!-- A translation rule that specifies a MAC address. you must use the same format that the client system uses -->
      <MACAddress></MACAddress>
      <!-- A translation rule that specifies a user name -->
      <User></User>
      <!-- The prefix string of the printer name when it is mapped to the remote desktop -->
      <!-- The printer name shows in remote desktop looks like "PrinterName (v1)" -->
      <PrinterName></PrinterName>
      <!-- The name of the driver that the printer uses -->
      <PrinterDriver></PrinterDriver>
      <!-- The IP address of the printer prepended with IP_ -->
      <IP></IP>
      <!-- Specify the port be used, by default is 9100 -->
      <Port></Port>
  </Policy>
</Root>
