
yast2 -i mysql mysql-client apache2 apache2-mod_php5
apache2 apache2-mod_php5
chmod 755 /usr/lib/apache2/mod_php5.so

wget ftp://rpmfind.net/linux/opensuse/distribution/11.1/repo/oss/suse/i586/libmm14-1.4.2-16.79.i586.rpm
rpm -i libmm14-1.4.2-16.79.i586.rpm

chkconfig -a mysql apache2
# Apache/PHP User
wwwrun:wwwrun

